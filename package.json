{
  "name": "portifolio-web-cypress",
  "version": "1.0.0",
  "description": "Portifólio de automação de teste e2e com cypress",
  "main": "index.js",
  "scripts": {
    "test:open": "cypress open --config-file ./cypress/config/development.js",
    "test:run": "cypress run --e2e --config-file ./cypress/config/homologacao.js --env includeSetup=true",
    "test:dev:chrome": "cypress run --e2e -b chrome --config-file ./cypress/config/development.js",
    "test:smokeTest": "cypress run --e2e --config-file ./cypress/config/homologacao.js --env grepTags=@smokeTest",
    "test:setup:auth": "cypress run --e2e --config-file ./cypress/config/homologacao.js --env includeSetup=true --spec cypress/e2e/00-setup/auth-setup.cy.js",
    "test:auth": "cypress run --e2e --config-file ./cypress/config/homologacao.js --spec cypress/e2e/access/auth.cy.js",
    "test:auth:with-setup": "cypress run --e2e --config-file ./cypress/config/homologacao.js --env includeSetup=true --spec \"cypress/e2e/00-setup/auth-setup.cy.js,cypress/e2e/access/auth.cy.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JessicaFortes/portifolio-web-cypress.git"
  },
  "keywords": [
    "cypress",
    "automacao",
    "web",
    "e2e",
    "teste"
  ],
  "author": "Jéssica Magalhães",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JessicaFortes/portifolio-web-cypress/issues"
  },
  "homepage": "https://github.com/JessicaFortes/portifolio-web-cypress#readme",
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "cypress-mochawesome-reporter": "^3.8.4"
  },
  "dependencies": {
    "cypress": "^15.10.0"
  }
}
